{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DatabaseConnection": "Server=(localdb)\\mssqllocaldb; Database=StudentEnrollmentDb; Trusted_Connection=True; MultipleActiveResultSets=True"
  },
  "JWTSettings": {
    "Key": "YOUR-SECRET-ENCODING-KEY",
    "Issuer": "https://localhost:44348",
    "Audience": "StudentEnrollmentAPI",
    "ExpiresInMinutes": 28800,
    "ExpiresInHours": 8,
    "ExpiresInDays": 1
  },
  "AzureStorageSettings": {
    "ConnectionString": "YOUR-ACCESS-KEY-STORAGE-CONNECTION-STRING",
    "StoragePath": "YOUR-PRIMARY-ENDPOINT-BLOB-SERVICE",
    "ContainerName": "YOUR-CONTAINER-NAME"
  }
}